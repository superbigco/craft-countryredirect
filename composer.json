{
  "name": "superbig/craft-countryredirect",
  "description": "Easily redirect visitors to a locale based on their country of origin",
  "type": "craft-plugin",
  "version": "3.0.0",
  "keywords": [
    "craft",
    "cms",
    "craftcms",
    "craft-plugin",
    "country redirect"
  ],
  "support": {
    "docs": "https://github.com/superbigco/craft-countryredirect/blob/master/README.md",
    "issues": "https://github.com/superbigco/craft-countryredirect/issues"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Superbig",
      "homepage": "https://superbig.co"
    }
  ],
  "require": {
    "craftcms/cms": "^4.0.0",
    "geoip2/geoip2": "^2.3",
    "jaybizzle/crawler-detect": "1.*",
    "jasongrimes/paginator": "^1.0.0",
    "ext-zlib": "*"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "autoload": {
    "psr-4": {
      "superbig\\countryredirect\\": "src/"
    }
  },
  "extra": {
    "name": "Country Redirect",
    "handle": "country-redirect",
    "hasCpSettings": true,
    "hasCpSection": false,
    "changelogUrl": "https://raw.githubusercontent.com/superbigco/craft-countryredirect/master/CHANGELOG.md",
    "class": "superbig\\countryredirect\\CountryRedirect"
  }
}
